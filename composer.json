{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/annotations": "^2.0.2",
    "doctrine/dbal": "^4.4.0",
    "doctrine/doctrine-bundle": "^2.14",
    "doctrine/doctrine-migrations-bundle": "^3.7.0",
    "doctrine/orm": "^3.3.2",
    "gesdinet/jwt-refresh-token-bundle": "^1.4",
    "lexik/jwt-authentication-bundle": "^3.1.1",
    "nelmio/cors-bundle": "^2.5",
    "phpdocumentor/reflection-docblock": "^5.6.5",
    "phpstan/phpdoc-parser": "^2.1",
    "symfony/asset": "7.3.0",
    "symfony/console": "7.3.6",
    "symfony/dotenv": "7.3.2",
    "symfony/expression-language": "7.3.2",
    "symfony/flex": "^2.5",
    "symfony/form": "7.3.6",
    "symfony/framework-bundle": "7.2.*",
    "symfony/http-foundation": "7.3.7",
    "symfony/mime": "7.3.4",
    "symfony/property-access": "7.3.3",
    "symfony/property-info": "7.3.5",
    "symfony/runtime": "7.3.4",
    "symfony/security-bundle": "7.3.4",
    "symfony/serializer": "8.0.2",
    "symfony/twig-bundle": "7.3.4",
    "symfony/validator": "7.3.7",
    "symfony/yaml": "7.3.5",
    "vich/uploader-bundle": "^2.8.1"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "bump-after-update": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/",
      "Stub\\": "tests/Stub/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.2.*"
    }
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^4.1",
    "phpunit/phpunit": "^12.4.2",
    "symfony/browser-kit": "7.3.6",
    "symfony/css-selector": "7.3.6",
    "symfony/maker-bundle": "^1.65.0",
    "symfony/phpunit-bridge": "^7.2"
  }
}
